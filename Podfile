# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'
use_frameworks!

#link_with 'ABA Music', 'ABA MusicTests'

def abaMusicPods
    # Pods for ABA Music
    pod 'AlamofireImage'
    pod 'CollectionViewSlantedLayout'
    pod 'PureLayout'
    pod 'SwiftyJSON', '~> 4.0'
    pod 'Cache'
end

target 'ABA Music' do
    abaMusicPods
end

target 'ABA MusicTests' do
    abaMusicPods
end
